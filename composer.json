{
  "name": "okrulik/laravel-phantomjs",
  "description": "Use phantom-js in laravel",
  "keywords": [
    "phantomjs-php",
    "phantomjs",
    "phantom-js",
    "laravel"
  ],
  "license": "MIT",
   "repositories": [
        { "type": "vcs", "url": "https://github.com/okrulik/php-phantomjs" }
    ],
  "support": {
    "issues": "https://github.com/MrJoshLab/laravel-phantomjs/issues",
    "source": "https://github.com/MrJoshLab/laravel-phantomjs"
  },
  "authors": [
    {
      "name": "Alireza Josheghani",
      "email": "josheghani.dev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Josh\\Component\\PhantomJs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "laravel/framework": "^6",
    "okrulik/php-phantomjs": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "extra": {
    "laravel": {
        "branch-alias": {
          "dev-develop": "^1.0"
        },
        "providers": [    
          "Josh\\Component\\PhantomJs\\PhantomJsServiceProvider"
        ],
        "aliases": {
            "PhantomJs": "Josh\\Component\\PhantomJs\\Facade\\PhantomJs"
        }
    }
  }
}
